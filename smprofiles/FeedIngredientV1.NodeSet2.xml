  <!-- FeedIngredientDataType - Structured DataType Definition -->
  <UADataType NodeId="ns=1;i=3001" BrowseName="1:FeedIngredientDataType">
    <DisplayName>FeedIngredientDataType</DisplayName>
    <Description>CESMII Smart Manufacturing Profile for Feed Ingredients used in manufacturing work orders. Defines the structure for ingredient materials including product identification, quantities, and weight measurements.</Description>
    <References>
      <Reference ReferenceType="HasSubtype" IsForward="false">Structure</Reference>
    </References>
    <Definition Name="1:FeedIngredientDataType">
      <Field Name="ProductID" DataType="i=12">
        <Description>Unique identifier for the feed ingredient product</Description>
      </Field>
      <Field Name="ProductNumber" DataType="i=8">
        <Description>Numeric product number for the feed ingredient</Description>
      </Field>
      <Field Name="ProductName" DataType="i=12">
        <Description>Human-readable name of the feed ingredient product</Description>
      </Field>
      <Field Name="LotNumber" DataType="i=12">
        <Description>Lot number for traceability of the feed ingredient batch</Description>
      </Field>
      <Field Name="UnitOfMeasure" DataType="i=12">
        <Description>Unit of measure for the quantity (e.g., CS for cases)</Description>
      </Field>
      <Field Name="Quantity" DataType="i=11">
        <Description>Quantity of the feed ingredient in the specified unit of measure</Description>
      </Field>
      <Field Name="WeightUnitOfMeasure" DataType="i=12">
        <Description>Unit of measure for weight (e.g., lb for pounds)</Description>
      </Field>
      <Field Name="Weight" DataType="i=11">
        <Description>Weight of the feed ingredient in the specified weight unit</Description>
      </Field>
    </Definition>
  </UADataType>

